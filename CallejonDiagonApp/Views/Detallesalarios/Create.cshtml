@model CallejonDiagonApp.Models.Detallesalario

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<h4>Detallesalario</h4>
<hr />
<div class="row">
