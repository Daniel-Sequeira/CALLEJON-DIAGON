@model IEnumerable<CallejonDiagonApp.Models.Detallesalario>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubtotalSalario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalSalario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdSalarioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubtotalSalario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalSalario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdSalarioNavigation.IdSalario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdDetalleSalario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdDetalleSalario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdDetalleSalario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
